{
  "name": "expo-aes-vetkeys",
  "version": "0.1.0",
  "description": "Hooks and components for using AES & vetkeys in Expo Web/Native applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "!**/__tests__/**"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "expo",
    "aes",
    "vetkeys",
    "react",
    "react-native"
  ],
  "author": "Yasuo Higa",
  "license": "MIT",
  "dependencies": {
    "expo-crypto-universal": "^0.1.0",
    "expo-storage-universal": "^0.3.0"
  },
  "peerDependencies": {
    "@dfinity/agent": "^0.20.0",
    "react": "^18.3.1",
    "react-native": "^0.76.7",
    "vetkeys-client-utils": "^0.1.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "^18.3.18",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8"
  }
}
